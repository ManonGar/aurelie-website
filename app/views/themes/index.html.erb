<div class="theme-cards">
  <% @themes.each do |theme| %>
      <%= link_to theme_path(theme) do %>
        <div class="theme-card" style="background-image: linear-gradient(rgb(195 161 161 / 0.2), rgb(148 108 108 / 0.2)), url('<%= cl_image_path theme.photo.key %>')">
        <%= theme.name.upcase %>
        </div>
      <% end %>
  <% end %>
</div>












